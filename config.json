{
  "name": "Bifrost",
  "version": "1.0",
  "slug": "bifrost",
  "description": "Bifrost add-on",
  "url": "https://github.com/joeblack2k/bifrost-hassio",
  "startup": "services",
  "arch": [
    "armhf",
    "armv7",
    "aarch64",
    "amd64",
    "i386"
  ],
  "boot": "auto",
  "ingress": true,
  "timeout": 30,
  "panel_icon": "mdi:bridge",
  "map": [
    "share:rw",
    "config:rw"
  ],
  "ports": {
    "8485/tcp": 8485
  },
  "ports_description": {
    "8485/tcp": "Websocket listener"
  },
  "options": {
    "mac_address": "00:11:22:33:44:55",
    "ipaddress": "10.12.0.20",
    "netmask": "255.255.255.0",
    "gateway": "10.12.0.1",
    "timezone": "Europe/Copenhagen",
    "z2m_url": "ws://10.0.0.100:8080"
  },
  "schema": {
    "mac_address": "str",
    "ipaddress": "str",
    "netmask": "str",
    "gateway": "str",
    "timezone": "str",
    "z2m_url": "str"
  }
}